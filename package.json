{
	"name": "hncode2",
	"displayName": "hncode2",
	"description": "Browse Hacker News in a tree view tab",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:top-stories"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "hncode",
					"title": "HNCodeÂ²",
					"icon": "resources/dark/HNCodeS.svg"
				}
			]
		},
		"views": {
			"hncode": [
				{
					"id": "top-stories",
					"name": "Top 500"
				},
				{
					"id": "ask-hn",
					"name": "Ask HN"
				}
			]
		},
		"commands": [
			{
				"command": "hncode.refresh",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "hncode.refresh",
					"when": "view == hncode",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"typed-rest-client": "^1.5.0"
	}
}
